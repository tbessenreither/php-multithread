{
    "name": "tbessenreither/php-multithread",
    "description": "A Symfony bundle providing multithreading capabilities for symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Bessenreither"
        }
    ],
    "require": {
        "php": "^8.4",
        "symfony/config": "^7.3",
        "symfony/dependency-injection": "^7.3",
        "symfony/http-kernel": "^7.3",
        "symfony/process": "^7.3",
        "symfony/uid": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "symfony/framework-bundle": "^7.3",
        "symfony/phpunit-bridge": "^7.3",
        "symfony/stopwatch": "^7.3",
        "symfony/console": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Tbessenreither\\PhpMultithread\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tbessenreither\\PhpMultithread\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "rm -f .phpunit.result.cache"
        ],
        "post-update-cmd": [
            "rm -f .phpunit.result.cache"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "platform": {
            "php": "8.4.0"
        }
    }
}